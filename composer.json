{
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org",
            "only": [
                "wpackagist-plugin/*",
                "wpackagist-theme/*"
            ]
        }
    ],
    "require-dev": {
        "roots/wordpress": "^6.8",
        "wp-cli/wp-cli-bundle": "^2.12"
    },
    "config": {
        "allow-plugins": {
            "roots/wordpress-core-installer": true,
            "composer/installers": true
        }
    },
    "scripts": {
        "wp" : "wp"
    },
    "require": {
        "wpackagist-theme/twentytwentyfive": "^1.3"
    }
}
